<!-- 데이터 저장빈 클래스 객체 별칭이름을 관리해 주는 파일이다 -->
<configuration>
	<typeAliases>
		<!-- EX) MemberVO 빈클래스 객체 별칭이름 member 를 지정
		<typeAlias type="net.daum.vo.MemberVO" alias="member" /> -->
	</typeAliases>
</configuration>